        <xs:element maxOccurs="1" minOccurs="0" name="pRetIR" type="TDec_0302"/>
        <!-- Valor  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vRetIR" type="TDec_1302"/>
        <!-- Valor da base de calculo de retencao de COFINS  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vBCCOFINS" type="TDec_1302"/>
        <!-- Percentual  -->
        <xs:element maxOccurs="1" minOccurs="0" name="pRetCOFINS" type="TDec_0302"/>
        <!-- Valor  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vRetCOFINS" type="TDec_1302"/>
        <!-- Valor da base de calculo de retencao de CSLL  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vBCCSLL" type="TDec_1302"/>
        <!-- Percentual  -->
        <xs:element maxOccurs="1" minOccurs="0" name="pRetCSLL" type="TDec_0302"/>
        <!-- Valor  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vRetCSLL" type="TDec_1302"/>
        <!-- Valor da base de calculo de retencao de PIS-PASEP  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vBCPISPASEP" type="TDec_1302"/>
        <!-- Percentual  -->
        <xs:element maxOccurs="1" minOccurs="0" name="pRetPISPASEP" type="TDec_0302"/>
        <!-- Valor  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vRetPISPASEP" type="TDec_1302"/>
        <!-- Valor total Aproximado de tributos federais -->
        <xs:element maxOccurs="1" minOccurs="0" name="totalAproxTribServ" type="TDec_1302"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para informações do ISS por substituicao tributaria em uma NFSe  -->
  <xs:element name="ISSST">
    <xs:complexType>
      <xs:sequence>
