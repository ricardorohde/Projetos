  <!-- Definicao da estrutura de dados para totalizacoes em uma NFSe  -->
  <xs:element name="total">
    <xs:complexType>
      <xs:sequence>
        <!-- Valor total de servico  -->
        <xs:element name="vServ" type="TDec_1302"/>
