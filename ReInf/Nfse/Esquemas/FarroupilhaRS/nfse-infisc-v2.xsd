<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="xmldsig-core-schema_v1.01.xsd"/>
  <!-- ***********************************************************  -->
  <!-- ****     XML para Prefeitura de Caxias do Sul - RS    *****  -->
  <!-- ****  NFS-e: Nota Fiscal de Serviços eletrônica       *****  -->
  <!-- **** Data da Última Atualização:  2014-03-28  16:23   *****  -->
  <!-- ***********************************************************  -->
  <!-- Definicao de estruturas para chamadas dos serviços Web       -->
  <!-- Definicao da estrutura de dados para Envio de Lote NFSe -->
  <xs:element name="envioLote">
    <xs:complexType>
      <xs:sequence>
        <!-- CNPJ de um contribuinte -->
        <xs:element name="CNPJ" type="TCnpj"/>
        <!-- Data e hora da transmissao do lote -->
        <xs:element name="dhTrans" type="TDataHora"/>
        <!-- Notas fiscais de servico do lote -->
        <xs:element maxOccurs="500" minOccurs="1" ref="NFS_e"/>
        <!-- Assinatura digital   -->
        <xs:element maxOccurs="1" minOccurs="0" ref="ds:Signature"/>
      </xs:sequence>
      <xs:attribute fixed="1.0" name="versao" type="xs:string" use="required"/>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para confirmacao de recebimento de Lote NFSe -->
  <xs:element name="confirmaLote">
    <xs:complexType>
      <xs:sequence>
        <!-- CNPJ de um contribuinte -->
        <xs:element name="CNPJ" type="TCnpj"/>
        <!-- Data e hora do recebimento do lote -->
        <xs:element name="dhRecbto" type="TDataHora"/>
        <!-- Situacao do lote -->
        <xs:element name="sit" type="TSituacaoLote"/>
        <!-- Assinatura digital   -->
        <xs:element maxOccurs="1" minOccurs="0" ref="ds:Signature"/>
      </xs:sequence>
      <xs:attribute fixed="1.0" name="versao" type="xs:string" use="required"/>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para Pedido de Status de Lote NFSe -->
  <xs:element name="pedidoStatusLote">
    <xs:complexType>
      <xs:sequence>
        <!-- CNPJ de um contribuinte -->
        <xs:element minOccurs="1" name="CNPJ" type="TCnpj"/>
        <!-- Número do lote   -->
        <xs:element minOccurs="1" name="cLote" type="TString15"/>
        <!-- Assinatura digital   -->
        <xs:element maxOccurs="1" minOccurs="0" ref="ds:Signature"/>
      </xs:sequence>
      <xs:attribute fixed="1.0" name="versao" type="xs:string" use="required"/>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para Pedido de Cancelamento de Lote NFSe -->
  <xs:element name="pedidoCancelamentoLote">
    <xs:complexType>
      <xs:sequence>
        <!-- CNPJ de um contribuinte -->
        <xs:element minOccurs="1" name="CNPJ" type="TCnpj"/>
        <!-- Número do lote   -->
        <xs:element minOccurs="1" name="cLote" type="TString15"/>
        <!-- Assinatura digital   -->
        <xs:element maxOccurs="1" minOccurs="0" ref="ds:Signature"/>
      </xs:sequence>
      <xs:attribute fixed="1.0" name="versao" type="xs:string" use="required"/>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para Pedido de um Lote NFSe -->
  <xs:element name="pedidoLoteNFSe">
    <xs:complexType>
      <xs:sequence>
        <!-- CNPJ de um contribuinte -->
        <xs:element minOccurs="1" name="CNPJ" type="TCnpj"/>
        <!-- Numero da nota inicial   -->
        <xs:element maxOccurs="1" minOccurs="0" name="notaInicial" type="TNF"/>
        <!-- Numero da nota final   -->
        <xs:element maxOccurs="1" minOccurs="0" name="notaFinal" type="TNF"/>
        <!-- Data de emissao inicial   -->
        <xs:element maxOccurs="1" minOccurs="0" name="emissaoInicial" type="TData"/>
        <!-- Data de emissao final   -->
        <xs:element maxOccurs="1" minOccurs="0" name="emissaoFinal" type="TData"/>
        <!-- Serie das notas   -->
        <xs:element minOccurs="1" name="serieNotaFiscal" type="TSerie"/>
        <!-- Assinatura digital   -->
        <xs:element maxOccurs="1" minOccurs="0" ref="ds:Signature"/>
      </xs:sequence>
      <xs:attribute fixed="1.0" name="versao" type="xs:string" use="required"/>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para Cancelamento de uma NFSe -->
  <xs:element name="pedCancelaNFSe">
    <xs:complexType>
      <xs:sequence>
        <!-- CNPJ de um contribuinte -->
        <xs:element minOccurs="1" name="CNPJ" type="TCnpj"/>
        <!-- Chave de acesso   -->
        <xs:element minOccurs="1" name="chvAcessoNFS_e" type="TChaveAcesso"/>
        <!-- Motivo  -->
        <xs:element name="motivo" type="TMotCanc"/>
        <!-- Assinatura digital   -->
        <xs:element maxOccurs="1" minOccurs="0" ref="ds:Signature"/>
      </xs:sequence>
      <xs:attribute fixed="1.0" name="versao" type="xs:string" use="required"/>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para Pedido de uma NFSe -->
  <xs:element name="pedidoNFSe">
    <xs:complexType>
      <xs:sequence>
        <!-- CNPJ de um contribuinte -->
        <xs:element minOccurs="1" name="CNPJ" type="TCnpj"/>
        <!-- Chave de acesso   -->
        <xs:element minOccurs="1" name="chvAcessoNFS_e" type="TChaveAcesso"/>
        <!-- Assinatura digital   -->
        <xs:element maxOccurs="1" minOccurs="0" ref="ds:Signature"/>
      </xs:sequence>
      <xs:attribute fixed="1.0" name="versao" type="xs:string" use="required"/>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para Consulta do Status de uma NFSe -->
  <xs:element name="pedConsultaTrans">
    <xs:complexType>
      <xs:sequence>
        <!-- CNPJ de um contribuinte -->
        <xs:element minOccurs="1" name="CNPJ" type="TCnpj"/>
        <!-- Chave de acesso   -->
        <xs:element minOccurs="1" name="chvAcessoNFS_e" type="TChaveAcesso"/>
        <!-- Assinatura digital   -->
        <xs:element maxOccurs="1" minOccurs="0" ref="ds:Signature"/>
      </xs:sequence>
      <xs:attribute fixed="1.0" name="versao" type="xs:string" use="required"/>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para Pedido da imagem de uma NFSe -->
  <xs:element name="pedidoNFSePNG">
    <xs:complexType>
      <xs:sequence>
        <!-- CNPJ de um contribuinte -->
        <xs:element minOccurs="1" name="CNPJ" type="TCnpj"/>
        <!-- Chave de acesso   -->
        <xs:element minOccurs="1" name="chvAcessoNFS_e" type="TChaveAcesso"/>
        <!-- Assinatura digital   -->
        <xs:element maxOccurs="1" minOccurs="0" ref="ds:Signature"/>
      </xs:sequence>
      <xs:attribute fixed="1.0" name="versao" type="xs:string" use="required"/>
    </xs:complexType>
  </xs:element>
  <!-- ***********************************************************  -->
  <!-- Definicao de estruturas de dados compostas                   -->
  <!-- Definicao da estrutura de dados para uma NFSe -->
  <xs:element name="NFS_e">
    <xs:complexType>
      <xs:sequence>
        <!-- Informacoes sobre uma nota -->
        <xs:element ref="infNFSe"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para as informacoes de uma NFSe -->
  <xs:element name="infNFSe">
    <xs:complexType>
      <xs:sequence>
        <!-- Dados de identificação da NFSe -->
        <xs:element ref="Id"/>
        <!-- Dados do emitente da NFSe -->
        <xs:element ref="emit"/>
        <!-- Dados de tomador da NFSe -->
        <xs:element ref="TomS"/>
        <!-- Dados do local de entrega  -->
        <xs:element maxOccurs="1" minOccurs="0" ref="dadosDaObra"/>
        <!-- Dados da transportadora  -->
        <xs:element maxOccurs="1" minOccurs="0" ref="transportadora"/>
        <!-- Detalhes dos itens de servico da NFSe -->
        <xs:element maxOccurs="999" minOccurs="1" ref="det"/>
        <!-- Dados de totalizacao da NFSe -->
        <xs:element ref="total"/>
        <!-- Dados de cobranca da NFSe -->
        <xs:element maxOccurs="1" minOccurs="0" ref="faturas"/>
        <!-- Local de tributacao a ser exibido na area de Informacoes Adicionais -->
        <xs:element maxOccurs="1" minOccurs="1" name="infAdicLT" type="TCodMunIBGE"/>
        <!-- Indica se deve ser exibido Exportacao de Servico na area de Informacoes Adicionais -->
        <xs:element maxOccurs="1" minOccurs="0" name="infAdicES" type="TSimNao"/>
        <!-- Informacoes adicionais de interesse do fisco -->
        <xs:element maxOccurs="999" minOccurs="0" name="infAdic" type="TString256"/>
        <!-- Exibir valor aproximado de tributos nas informações adicionais -->
        <xs:element maxOccurs="1" minOccurs="0" name="infAdicAT" type="TCharInt"/>
      </xs:sequence>
      <!-- Versão do formato XML da NFSe em está sendo submetido o arquivo XML de lote -->
      <xs:attribute name="versao" type="xs:string" use="required"/>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para a identificacao de uma NFSe -->
  <xs:element name="Id">
    <xs:complexType>
      <xs:sequence>
        <!-- Codigo numerico aleatorio para compor chave de acesso  -->
        <xs:element name="cNFS_e" type="TNumAleatorio"/>
        <!-- Natureza Operação de uma NFSe -->
        <xs:element maxOccurs="1" minOccurs="0" name="natOp" type="TString60"/>
        <!-- Modelo de uma NFSe -->
        <xs:element name="mod" type="TModelo"/>
        <!-- Serie da nota  -->
        <xs:element name="serie" type="TSerie"/>
        <!-- Numero da nota  -->
        <xs:element name="nNFS_e" type="TNF"/>
        <!-- Data de emissao -->
        <xs:element name="dEmi" type="TData"/>
        <!-- Hora de emissao -->
        <xs:element name="hEmi" type="THora"/>
        <!-- Tipo de uma NFSe -->
        <xs:element name="tpNF" type="TNFSe"/>
        <!-- Chave de acesso de uma NFSe -->
        <xs:element name="refNF" type="TChaveAcesso"/>
        <!-- Codigo do municipio -->
        <xs:element maxOccurs="1" minOccurs="0" name="cMunFG" type="TCodMunIBGE"/>
        <!-- Formato de impressao de uma NFSe: 1-Retrato, 2-Paisagem -->
        <xs:element maxOccurs="1" minOccurs="0" name="tpImp" type="TImpressao"/>
        <!-- Formato de emissao de uma NFSe: N-Normal, C-Contigencia -->
        <xs:element name="tpEmis" type="TEmissao"/>
        <!-- NFSe cancelada (S) ou não (N) -->
        <xs:element maxOccurs="1" minOccurs="0" name="cancelada" type="TSimNao"/>
        <!-- Motivo do cancelamento  -->
        <xs:element maxOccurs="999" minOccurs="0" name="motCanc" type="TMotCanc"/>
        <!-- Data do cancelamento -->
        <xs:element maxOccurs="1" minOccurs="0" name="dataCanc" type="TData"/>
        <!-- Numero da nota substitutiva da anulada/cancelada -->
        <xs:element maxOccurs="1" minOccurs="0" name="notaSub" type="TNF"/>
        <!-- Incluir canhoto na imagem da nota: 0 nao incluir, 1 incluir no topo, 2 incluir no rodape -->
        <xs:element maxOccurs="1" minOccurs="0" name="canhoto" type="TCharInt"/>
        <!-- Numero da RPS que originou a nota -->
        <xs:element maxOccurs="1" minOccurs="0" name="rps" type="TNF"/>
        <!-- Serie da RPS que originou a nota  -->
        <xs:element maxOccurs="1" minOccurs="0" name="serierps" type="TSerie"/>
        <!-- Codigo de validacao do RPS (USO INTERNO) -->
        <xs:element maxOccurs="1" minOccurs="0" name="codRPS" type="TString15"/>
        <!-- Ambiente de emissao da NFSe: 1 producao ou 2 homologacao -->
        <xs:element name="ambienteEmi" type="TCharInt"/>
        <!-- Forma de emissao da NFSe: 1 via Portal, 2 via servicos Web, 3 via upload ou 4 via RPS -->
        <xs:element name="formaEmi" type="TCharInt"/>
        <!-- Nota de empreitada global: 1 caso seja ou 2 caso contrario -->
        <xs:element name="empreitadaGlobal" type="TCharInt"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para o emitente de uma NFSe -->
  <xs:element name="emit">
    <xs:complexType>
      <xs:sequence>
        <!-- CNPJ   -->
        <xs:element name="CNPJ" type="TCnpj"/>
        <!-- Nome ou razao social -->
        <xs:element name="xNome" type="TString150"/>
        <!-- Nome fantasia -->
        <xs:element maxOccurs="1" minOccurs="0" name="xFant" type="TString60"/>
        <!-- Inscricao municipal  -->
        <xs:element name="IM" type="TString15"/>
        <!-- endereço de correio eletrônico do emitente  -->
        <xs:element maxOccurs="1" minOccurs="0" name="xEmail" type="TString50"/>
        <!-- URL do site do emitente  -->
        <xs:element maxOccurs="1" minOccurs="0" name="xSite" type="TString50"/>
        <!-- Endereco do emitente  -->
        <xs:element ref="end"/>
        <!-- Fone  -->
        <xs:element maxOccurs="1" minOccurs="0" name="fone" type="TFone"/>
        <!-- Fone alternativo -->
        <xs:element maxOccurs="1" minOccurs="0" name="fone2" type="TFone"/>
        <!-- Inscricao Estadual  -->
        <xs:element maxOccurs="1" minOccurs="0" name="IE" type="TString15"/>
        <!-- Regime tributario  -->
        <xs:element name="regimeTrib" type="TCharInt"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para endereco de um emitente de uma NFSe -->
  <xs:element name="end">
    <xs:complexType>
      <xs:sequence>
        <!-- Logradouro  -->
        <xs:element name="xLgr" type="TString100"/>
        <!-- Numero  -->
        <xs:element name="nro" type="TString6"/>
        <!-- Complemento  -->
        <xs:element maxOccurs="1" minOccurs="0" name="xCpl" type="TString100"/>
        <!-- Bairro  -->
        <xs:element name="xBairro" type="TString100"/>
        <!-- Codigo do municipio -->
        <xs:element name="cMun" type="TCodMunIBGE"/>
        <!-- Nome do municipio -->
        <xs:element name="xMun" type="TString60"/>
        <!-- UF -->
        <xs:element name="UF" type="TString2"/>
        <!-- CEP  -->
        <xs:element name="CEP" type="TCep"/>
        <!-- Codigo do Pais   -->
        <xs:element name="cPais" type="TcPais"/>
        <!-- Pais  -->
        <xs:element name="xPais" type="TString100"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para tomador de uma NFSe -->
  <xs:element name="TomS">
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <!-- CNPJ de um contribuinte -->
          <xs:element name="CNPJ" type="TCnpj"/>
          <!-- CPF de um contribuinte -->
          <xs:element name="CPF" type="TCpf"/>
        </xs:choice>
        <!-- Nome ou razao social -->
        <xs:element maxOccurs="1" minOccurs="0" name="xNome" type="TString100"/>
        <!-- Endereco -->
        <xs:element ref="ender"/>
        <!-- Endereco de correio eletronico -->
        <xs:element maxOccurs="1" minOccurs="0" name="xEmail" type="TString120"/>
        <!-- Inscricao Estadual  -->
        <xs:element maxOccurs="1" minOccurs="0" name="IE" type="TString15"/>
        <!-- Inscricao municipal  -->
        <xs:element maxOccurs="1" minOccurs="0" name="IM" type="TString15"/>
        <!-- Inscricao municipal eventual -->
        <xs:element maxOccurs="1" minOccurs="0" name="IME" type="TString15"/>
        <!-- Praca -->
        <xs:element maxOccurs="1" minOccurs="0" name="Praca" type="TString60"/>
        <!-- Fone  -->
        <xs:element maxOccurs="1" minOccurs="0" name="fone" type="TFone"/>
        <!-- Fone alternativo  -->
        <xs:element maxOccurs="1" minOccurs="0" name="fone2" type="TFone"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para endereco de um tomador de uma NFSe -->
  <xs:element name="ender">
    <xs:complexType>
      <xs:sequence>
        <!-- Logradouro  -->
        <xs:element maxOccurs="1" minOccurs="0" name="xLgr" type="TString100"/>
        <!-- Numero  -->
        <xs:element maxOccurs="1" minOccurs="0" name="nro" type="TString6"/>
        <!-- Complemento  -->
        <xs:element maxOccurs="1" minOccurs="0" name="xCpl" type="TString100"/>
        <!-- Bairro  -->
        <xs:element maxOccurs="1" minOccurs="0" name="xBairro" type="TString100"/>
        <!-- Codigo do municipio   -->
        <xs:element name="cMun" type="TCodMunIBGE"/>
        <!-- Nome do municipio -->
        <xs:element name="xMun" type="TString60"/>
        <!-- UF -->
        <xs:element name="UF" type="TString2"/>
        <!-- CEP -->
        <xs:element maxOccurs="1" minOccurs="0" name="CEP" type="TCep"/>
        <!-- Codigo de pais  -->
        <xs:element name="cPais" type="TcPais"/>
        <!-- Pais  -->
        <xs:element name="xPais" type="TString100"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para endereco de dados da obra -->
  <xs:element name="dadosDaObra">
    <xs:complexType>
      <xs:sequence>
        <!-- Logradouro  -->
        <xs:element name="xLogObra" type="TString100"/>
        <!-- Complemento  -->
        <xs:element maxOccurs="1" minOccurs="0" name="xComplObra" type="TString100"/>
        <!-- Numero  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vNumeroObra" type="TString15"/>
        <!-- Bairro  -->
        <xs:element maxOccurs="1" minOccurs="0" name="xBairroObra" type="TString100"/>
        <!-- CEP -->
        <xs:element maxOccurs="1" minOccurs="0" name="xCepObra" type="TCep"/>
        <!-- Codigo do municipio -->
        <xs:element name="cCidadeObra" type="TCodMunIBGE"/>
        <!-- Nome do municipio -->
        <xs:element name="xCidadeObra" type="TString60"/>
        <!-- UF -->
        <xs:element name="xUfObra" type="TString2"/>
        <!-- Codigo do Pais  -->
        <xs:element name="cPaisObra" type="TcPais"/>
        <!-- Nome do Pais  -->
        <xs:element name="xPaisObra" type="TString100"/>
        <!-- Numero da ART em uma NFSe de construcao civil -->
        <xs:element maxOccurs="1" minOccurs="0" name="numeroArt" type="TString12"/>
        <!-- Numero da matricula no CEI em uma NFSe de construcao civil -->
        <xs:element maxOccurs="1" minOccurs="0" name="numeroCei" type="TString12"/>
        <!-- Numero do projeto em uma NFSe de construcao civil -->
        <xs:element maxOccurs="1" minOccurs="0" name="numeroProj" type="TString15"/>
        <!-- Numero da matricula em uma NFSe de construcao civil -->
        <xs:element maxOccurs="1" minOccurs="0" name="numeroMatri" type="TString15"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para transportadora -->
  <xs:element name="transportadora">
    <xs:complexType>
      <xs:sequence>
        <!-- Nome da transportadora  -->
        <xs:element name="xNomeTrans" type="TString100"/>
        <!-- CPF/CNPJ da transportadora  -->
        <xs:element maxOccurs="1" minOccurs="0" name="xCpfCnpjTrans" type="TString15"/>
        <!-- Inscricao Estadual da transportadora  -->
        <xs:element maxOccurs="1" minOccurs="0" name="xInscEstTrans" type="TString15"/>
        <!-- Placa do automovel da transportadora  -->
        <xs:element maxOccurs="1" minOccurs="0" name="xPlacaTrans" type="TPlacaVeic"/>
        <!-- Endereco da transportadora -->
        <xs:element maxOccurs="1" minOccurs="0" name="xEndTrans" type="TString100"/>
        <!-- Codigo do Municipio da transportadora -->
        <xs:element maxOccurs="1" minOccurs="0" name="cMunTrans" type="TCodMunIBGE"/>
        <!-- Nome do Municipio da transportadora -->
        <xs:element maxOccurs="1" minOccurs="0" name="xMunTrans" type="TString60"/>
        <!-- UF da transportadora  -->
        <xs:element maxOccurs="1" minOccurs="0" name="xUfTrans" type="TString2"/>
        <!-- Codigo do Pais da transportadora -->
        <xs:element maxOccurs="1" minOccurs="0" name="cPaisTrans" type="TcPais"/>
        <!-- Nome do Pais da transportadora -->
        <xs:element maxOccurs="1" minOccurs="0" name="xPaisTrans" type="TString100"/>
        <!-- Tipo do frete  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vTipoFreteTrans" type="TFrete"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para detalhe do servico de uma NFSe -->
  <xs:element name="det">
    <xs:complexType>
      <xs:sequence>
        <!-- Ordem sequencial do item na nota  -->
        <xs:element name="nItem" type="TOrdemSeq"/>
        <xs:element ref="serv"/>
        <xs:element maxOccurs="1" minOccurs="0" ref="ISSST"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para detalhe de item de servico de uma NFSe -->
  <xs:element name="serv">
    <xs:complexType>
      <xs:sequence>
        <!-- Codigo municipal do servico  -->
        <xs:element name="cServ" type="TString60"/>
        <!-- Codigo do servico conforme Lei 116  -->
        <xs:element name="cLCServ" type="TLCServ"/>
        <!-- Descricao do serviço  -->
        <xs:element name="xServ" type="TString256"/>
        <!-- Local de tributação do servico -->
        <xs:element maxOccurs="1" minOccurs="0" name="localTributacao" type="TCodMunIBGE"/>
        <!-- Local da verificação do resultado do servico  -->
        <xs:element name="localVerifResServ" type="TCharInt"/>
        <!-- Unidade do servico  -->
        <xs:element name="uTrib" type="TString2"/>
        <!-- Quantidade do servico -->
        <xs:element name="qTrib" type="TDec_1302"/>
        <!-- Valor unitario -->
        <xs:element name="vUnit" type="TDec_1302"/>
        <!-- Valor do servico -->
        <xs:element name="vServ" type="TDec_1302"/>
        <!-- Valor de desconto -->
        <xs:element maxOccurs="1" minOccurs="0" name="vDesc" type="TDec_1302"/>
        <!-- Valor da base de calculo de ISSQN  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vBCISS" type="TDec_1302"/>
        <!-- Aliquota do ISSQN -->
        <xs:element maxOccurs="1" minOccurs="0" name="pISS" type="TDec_0302"/>
        <!-- Valor do ISSQN -->
        <xs:element maxOccurs="1" minOccurs="0" name="vISS" type="TDec_1302"/>
        <!-- Valor da base de calculo de retencao de INSS  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vBCINSS" type="TDec_1302"/>
        <!-- Percentual de retencao de INSS do item de servico  -->
        <xs:element maxOccurs="1" minOccurs="0" name="pRetINSS" type="TDec_1302"/>
        <!-- Valor de retencao de INSS do item de servico  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vRetINSS" type="TDec_1302"/>
        <!-- Valor de redução da base de calculo  -->
