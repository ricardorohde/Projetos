        <xs:element maxOccurs="1" minOccurs="0" ref="Ret"/>
        <!-- Valor liquido total das faturas -->
        <xs:element maxOccurs="1" minOccurs="0" name="vtLiqFaturas" type="TDec_1302"/>
        <!-- Totalizacoes do ISS -->
        <xs:element ref="ISS"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para valores do ISSQN em uma NFSe  -->
  <xs:element name="ISS">
    <xs:complexType>
      <xs:sequence>
        <!-- Valor da base de calculo de ISSQN  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vBCISS" type="TDec_1302"/>
        <!-- Valor do ISSQN  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vISS" type="TDec_1302"/>
        <!-- Valor da base de calculo de ISSQN por substituicao tributaria  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vBCSTISS" type="TDec_1302"/>
        <!-- Valor do ISSQN por substituicao tributaria  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vSTISS" type="TDec_1302"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para retencoes em uma NFSe  -->
  <xs:element name="Ret">
    <xs:complexType>
      <xs:sequence>
        <!-- Valor total da retencao de IR -->
        <xs:element maxOccurs="1" minOccurs="0" name="vRetIR" type="TDec_1302"/>
