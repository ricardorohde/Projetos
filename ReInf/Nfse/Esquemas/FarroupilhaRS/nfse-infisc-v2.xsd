        <xs:element maxOccurs="1" minOccurs="0" name="vRetCOFINS" type="TDec_1302"/>
        <!-- Valor total da retencao de CSLL -->
        <xs:element maxOccurs="1" minOccurs="0" name="vRetCSLL" type="TDec_1302"/>
        <!-- Valor total da retencao de INSS  -->
        <xs:element maxOccurs="1" minOccurs="0" name="vRetINSS" type="TDec_1302"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para informacoes de faturas em uma NFSe  -->
  <xs:element name="faturas">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="999" minOccurs="1" ref="fat"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Definicao da estrutura de dados para informacoes sobre faturas em uma NFSe  -->
  <xs:element name="fat">
