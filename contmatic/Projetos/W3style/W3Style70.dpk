  DirectoryDialog in 'DirectoryDialog.pas' {fmDirectoryDialog},
  ITDBSearch in 'ITDBSearch.pas',
  ITDBFileSearch in 'ITDBFileSearch.pas',
  ITDBDirectorySearch in 'ITDBDirectorySearch.pas',
  ItDatabase in 'ItDatabase.pas',
  ITMenuItem in 'ITMenuItem.pas',
  ITFilterMemo in 'ITFilterMemo.pas',
