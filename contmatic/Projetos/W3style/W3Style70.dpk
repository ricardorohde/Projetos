  ITValida in 'ITValida.pas',
  DeclaraType in 'DeclaraType.pas',
  LookupEdit in 'LookupEdit.pas',
  dbLookUpEdit in 'dbLookUpEdit.pas',
  DBFindEdit in 'DBFindEdit.pas',
  DBSearchEdit in 'DBSearchEdit.pas',
  UConfiguracao in 'UConfiguracao.pas' {fmConfiguracao};

end.
